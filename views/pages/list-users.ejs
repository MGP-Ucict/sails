<div id="list-users" v-cloak>
  <div class="container">
    <h1>List users</h1>
    <hr/>
   
  <ul>
  <li v-for="user in users">
{{user.fullName}}
<div v-for="r in formData.rights">
 <div v-if="'/edit-user/:id'===r">
<a class="btn btn-outline-info" :href="'/edit-user/'+ user.id"> Edit </a>
</div>	
</div>
<div v-for="r in formData.rights">
 <div v-if="'/edit-user/:id'===r">
<a class="btn btn-outline-danger" :href="'/del-user/'+ user.id"> Delete </a>	
</div>	
</div>
	
  </li>
</ul>
  </div>
</div>
